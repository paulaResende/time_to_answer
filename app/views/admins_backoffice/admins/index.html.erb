<h1>Listando administradores</h1>
<%= @admins.inspect %>

<table class="table table-striped table-bordered table-hover">
  <thead>
  <tr>
      <th>ID</th>
      <th>Email</th>
      </tr>
      </thead>
      <tbody>
      <%  @admins.each do |admin| %>
        <tr>
        <td><%= admin.id %></td>
        <td><%= admin.email %></td>
        </tr>
      <% end %>
      </tbody>
      </table>
